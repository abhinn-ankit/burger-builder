{
    "printWidth": 120,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "semi": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "overrides": [{
        "files": "*.json",
        "options": {
            "tabWidth": 2,
            "singleQuote": false
        }
    }, {
        "files": ".prettierrc",
        "options": {
            "parser": "typescript"
        }
    }]
}